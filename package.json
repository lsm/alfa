{
  "name": "alfa",
  "version": "0.4.2",
  "description": "",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build-watch": "cross-env BABEL_ENV=commonjs babel src --watch --out-dir lib",
    "nyc": "BABEL_ENV=development nyc ava",
    "lint": "eslint src",
    "test": "BABEL_ENV=development ava --verbose",
    "watch": "BABEL_ENV=development ava --watch --verbose",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "update-snapshots": "BABEL_ENV=development ava --update-snapshots"
  },
  "author": "Marc Liu <m@liu.pm> (https://github.com/lsm)",
  "license": "MIT",
  "devDependencies": {
    "ava": "0.21.0",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "coveralls": "2.13.1",
    "cross-env": "5.0.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "1.0.5",
    "eslint-plugin-class-property": "1.0.6",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.1.0",
    "nyc": "11.0.3",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-test-renderer": "15.6.1"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "source": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
