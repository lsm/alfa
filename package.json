{
  "name": "alfa",
  "version": "0.6.0",
  "description": "Effortless React State Management.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "src",
    "dist"
  ],
  "scripts": {
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env BABEL_ENV=rollup NODE_ENV=development rollup -c -o dist/alfa.js",
    "build:umd:min": "cross-env BABEL_ENV=rollup NODE_ENV=production rollup -c -o dist/alfa.min.js",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd && npm run build:umd:min",
    "build-watch": "cross-env BABEL_ENV=commonjs babel src --watch --out-dir lib",
    "nyc": "BABEL_ENV=development TEST_ENV=unit nyc babel-node test/unit.js",
    "lint": "eslint src",
    "test": "BABEL_ENV=development TEST_ENV=unit babel-node test/unit.js",
    "watch": "BABEL_ENV=development TEST_ENV=unit babel-watch test/unit.js",
    "report": "nyc report --reporter=html",
    "watch-report": "nodemon -w src -w test --exec BABEL_ENV=development TEST_ENV=unit nyc --reporter=html babel-node test/unit.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Marc Liu <m@liu.pm> (https://github.com/lsm)",
  "license": "MIT",
  "homepage": "https://github.com/lsm/alfa",
  "repository": "github:lsm/alfa",
  "bugs": "https://github.com/lsm/alfa/issues",
  "dependencies": {
    "isobject": "^3.0.1",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-watch": "2.0.7",
    "coveralls": "3.0.1",
    "cross-env": "5.1.6",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-class-property": "1.1.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.9.1",
    "jsdom": "11.11.0",
    "karma": "2.0.2",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-opera-launcher": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sauce-launcher": "git+https://github.com/edoardocavazza/karma-sauce-launcher.git#chialab",
    "karma-sourcemap-loader": "0.3.7",
    "karma-tap": "4.1.4",
    "karma-webpack": "3.0.0",
    "nodemon": "1.17.5",
    "nyc": "12.0.2",
    "prettier": "1.13.4",
    "raf": "3.4.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-test-renderer": "16.4.0",
    "rollup": "0.60.0",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "4.0.0",
    "tape": "4.9.0",
    "webpack": "4.11.1"
  }
}
