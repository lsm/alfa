{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": ["> 0.25%", "not dead"]
      }
    }],
    "@babel/react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-class-properties"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    }
  }
}
