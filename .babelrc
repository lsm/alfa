{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["> 1%"]
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread", "@babel/plugin-proposal-class-properties"],
  "env": {
    "development": {
      "sourceMaps": "inline"
    }
  }
}
